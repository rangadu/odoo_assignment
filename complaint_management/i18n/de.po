#. module: complaint_management
#: model:mail.template,body_html:complaint_management.complaint_close_email_template
msgid ""
"<div>\n"
"                    Dear <t t-out=\"object.sudo().partner_id.name or 'Madam/Sir'\">Madam/Sir</t>,<br><br>\n"
"                    We would like to inform you that we have closed your ticket.<br><br>\n"
"                    The reference for your complaint is <strong><t t-out=\"object.name or ''\">0</t></strong>.<br><br>\n"
"                    <h3>Complaint Details</h3><br>\n"
"                    <h4>Type</h4> <p><t t-out=\"object.complaint_type_id.name\"></t></p><br>\n"
"                    <h4>Complaint</h4>\n"
"                    <p><t t-out=\"object.description\"></t></p><br>\n"
"                    <t t-if=\"object.is_question\">\n"
"                        <h4>Answer</h4>\n"
"                        <p><t t-out=\"object.answer\"></t></p><br>\n"
"                    </t>\n"
"                    <h4>Action Plan</h4>\n"
"                    <p><t t-out=\"object.action_plan\"></t></p><br>\n"
"                    Best regards,<br><br>\n"
"                    Support Team,\n"
"                    RealEstateX\n"
"                </div>\n"
"            "
msgstr ""
"<div>\n"
"                    Liebling <t t-out=\"object.sudo().partner_id.name or 'Frau/Herr'\">Frau/Herr</t>,<br><br>\n"
"                    Wir möchten Sie darüber informieren, dass wir Ihr Ticket geschlossen haben.<br><br>\n"
"                    Die Referenz für Ihre Beschwerde lautet <strong><t t-out=\"object.name or ''\">0</t></strong>.<br><br>\n"
"                    <h3>Einzelheiten zur Beschwerde</h3><br>\n"
"                    <h4>Typ</h4> <p><t t-out=\"object.complaint_type_id.name\"></t></p><br>\n"
"                    <h4>Beschwerde</h4>\n"
"                    <p><t t-out=\"object.description\"></t></p><br>\n"
"                    <t t-if=\"object.is_question\">\n"
"                        <h4>Antwort</h4>\n"
"                        <p><t t-out=\"object.answer\"></t></p><br>\n"
"                    </t>\n"
"                    <h4>Aktionsplan</h4>\n"
"                    <p><t t-out=\"object.action_plan\"></t></p><br>\n"
"                    Beste grüße,<br><br>\n"
"                    Support-Team,\n"
"                    RealEstateX\n"
"                </div>\n"
"            "

#. module: complaint_management
#: model:mail.template,body_html:complaint_management.complaint_request_email_template
msgid ""
"<div>\n"
"                    Dear <t t-out=\"object.sudo().partner_id.name or 'Madam/Sir'\">Madam/Sir</t>,<br><br>\n"
"                    Your complaint has been received and is being reviewed by our Support team.<br><br>\n"
"                    The reference for your complaint is <strong><t t-out=\"object.name or ''\">0</t></strong>.<br><br>\n"
"\n"
"                    Best regards,<br><br>\n"
"                    Support Team,\n"
"                    RealEstateX\n"
"                </div>\n"
"            "
msgstr ""
"<div>\n"
"                    Liebling <t t-out=\"object.sudo().partner_id.name or 'Frau/Herr'\">Frau/Herr</t>,<br><br>\n"
"                    Ihre Beschwerde ist eingegangen und wird von unserem Support-Team geprüft.<br><br>\n"
"                    Die Referenz für Ihre Beschwerde lautet <strong><t t-out=\"object.name or ''\">0</t></strong>.<br><br>\n"
"\n"
"                    Beste grüße,<br><br>\n"
"                    Support-Team,\n"
"                    RealEstateX\n"
"                </div>\n"
"            "

